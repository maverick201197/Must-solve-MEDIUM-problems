
#include <iostream>
using namespace std;
int main()
{
int a, b, i, flag;
cout << "enter the start value" <<endl;
cin >> a;
cout << "Enter end value" <<endl;
cin >> b;

while (a < b)
{
flag = 0;
for(i = 2; i <= a/2; ++i)
{
if(a % i == 0)
{
flag = 1;
break;
}
}
if (flag == 0)
cout << a<<endl;
++a;
}
cout << endl;
return 0;
}


enter the start value
20 50
